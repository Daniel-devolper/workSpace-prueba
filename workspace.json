{
  "version": 2,
  "projects": {
    "booking": {
      "root": "apps/booking",
      "sourceRoot": "apps/booking",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/booking",
            "outputPath": "dist/apps/booking"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "booking:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "booking:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "booking:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/booking"],
          "options": {
            "jestConfig": "apps/booking/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/booking/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "ecomerce": {
      "root": "apps/ecomerce",
      "sourceRoot": "apps/ecomerce",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/ecomerce",
            "outputPath": "dist/apps/ecomerce"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "ecomerce:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "ecomerce:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "ecomerce:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ecomerce"],
          "options": {
            "jestConfig": "apps/ecomerce/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/ecomerce/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "ecommerce-gatsby": {
      "root": "apps/ecommerce-gatsby",
      "sourceRoot": "apps/ecommerce-gatsby/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/gatsby:build",
          "options": {
            "outputPath": "apps/ecommerce-gatsby/public",
            "uglify": true,
            "color": true,
            "profile": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/gatsby:server",
          "options": {
            "buildTarget": "ecommerce-gatsby:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "ecommerce-gatsby:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/ecommerce-gatsby/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ecommerce-gatsby"],
          "options": {
            "jestConfig": "apps/ecommerce-gatsby/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "visitors": {
      "projectType": "application",
      "root": "apps/visitors",
      "sourceRoot": "apps/visitors/src",
      "prefix": "example",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/visitors",
            "index": "apps/visitors/src/index.html",
            "main": "apps/visitors/src/main.ts",
            "polyfills": "apps/visitors/src/polyfills.ts",
            "tsConfig": "apps/visitors/tsconfig.app.json",
            "assets": [
              "apps/visitors/src/favicon.ico",
              "apps/visitors/src/assets"
            ],
            "styles": ["apps/visitors/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/visitors/src/environments/environment.ts",
                  "with": "apps/visitors/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "visitors:build:production"
            },
            "development": {
              "browserTarget": "visitors:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "visitors:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/visitors/src/**/*.ts",
              "apps/visitors/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/visitors"],
          "options": {
            "jestConfig": "apps/visitors/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "visitors-gatsby": {
      "root": "apps/visitors-gatsby",
      "sourceRoot": "apps/visitors-gatsby/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/gatsby:build",
          "options": {
            "outputPath": "apps/visitors-gatsby/public",
            "uglify": true,
            "color": true,
            "profile": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/gatsby:server",
          "options": {
            "buildTarget": "visitors-gatsby:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "visitors-gatsby:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/visitors-gatsby/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/visitors-gatsby"],
          "options": {
            "jestConfig": "apps/visitors-gatsby/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    },
    "@nrwl/gatsby": {
      "application": {
        "style": "css"
      }
    }
  },
  "defaultProject": "ecomerce"
}
